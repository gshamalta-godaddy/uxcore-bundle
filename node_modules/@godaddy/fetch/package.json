{
  "name": "@godaddy/fetch",
  "version": "1.0.4",
  "description": "Cross-platform Fetch API",
  "main": "./node.js",
  "browser": "./browser.js",
  "react-native": "./react-native.js",
  "scripts": {
    "lint": "eslint-godaddy \"{*.js,test/*.js}\"",
    "test": "nyc --reporter=text --reporter=json-summary npm run test:all",
    "test:all": "npm run test:browser && npm run test:node && npm run test:react-native",
    "test:node": "mocha test/node.js",
    "test:browser": "mocha --require setup-env test/browser.js",
    "test:react-native": "echo 'not implemented'",
    "posttest": "npm run lint"
  },
  "maintainers": [
    "Martijn Swaagman <mswaagman@godaddy.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.secureserver.net:javascript/fetch.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.secureserver.net/artifactory/api/npm/node-jtsc-local/"
  },
  "keywords": [
    "fetch",
    "cross",
    "platform"
  ],
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "extends browserslist-config-godaddy"
  ],
  "author": "GoDaddy.com Operating Company LLC",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "assume": "^2.2.0",
    "browserslist-config-godaddy": "^1.0.1",
    "eslint": "^6.2.1",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.1.0",
    "jsdom": "^15.1.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "setup-env": "^1.2.2",
    "whatwg-fetch": "^3.0.0"
  }
}
