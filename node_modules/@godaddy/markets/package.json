{
  "name": "@godaddy/markets",
  "version": "3.2.1",
  "description": "Shared configuration library for market, language, and currency data across GoDaddy",
  "main": "lib/markets.js",
  "browser": "lib/markets.js",
  "module": "src/markets.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run test-mocha",
    "test-mocha": "nyc --reporter=lcov --reporter=text mocha --require @babel/register",
    "test-debug": "npm run lint && mocha --require @babel/register --debug-brk",
    "lint": "eslint-godaddy -c .eslintrc test/*.js src/*.js",
    "build": "babel --out-dir lib src",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.secureserver.net/javascript/markets.git"
  },
  "keywords": [
    "market",
    "language",
    "locale",
    "currency"
  ],
  "author": "GoDaddy.com, LLC",
  "contributors": [
    "Jonathan Keslin <jkeslin@godaddy.com>",
    "Yiding Wang <ywang@godaddy.com",
    "Bradley C Bailey <bcbailey@godaddy.com>",
    "Matthew Bennett <mtbennett@godaddy.com>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@godaddy/request": "^3.0.2",
    "lru-cache": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/register": "^7.9.0",
    "@ux/babel-config": "^2000.7.7",
    "assume": "^2.2.0",
    "babelify": "^10.0.0",
    "eslint": "^7.11.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-mocha": "^8.0.0",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.0"
  },
  "publishConfig": {
    "registry": "https://artifactory.secureserver.net/artifactory/api/npm/node-jtsc-local/"
  },
  "engines": {
    "node": ">= 10.x.x"
  }
}
