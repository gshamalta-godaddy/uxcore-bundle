{
  "name": "@godaddy/request",
  "version": "3.0.3",
  "description": "Client and server side library to perform XHR requests.",
  "main": "lib/index.js",
  "browser": "lib/index.js",
  "module": "./index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "babel index.js -d ./lib",
    "test": "mochify --web-security=false --reporter spec ./test/*.test.js",
    "posttest": "npm run lint",
    "lint": "eslint --fix test/*.js *.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.secureserver.net/javascript/request.git"
  },
  "keywords": [
    "request",
    "requests",
    "xhr",
    "node",
    "browser",
    "ajax"
  ],
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "author": "GoDaddy.com, LLC",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.0.0",
    "assume": "^2.2.0",
    "babelify": "^10.0.0",
    "eslint": "^6.2.2",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^6.1.0",
    "mocha": "^6.1.4",
    "mochify": "^6.3.0"
  },
  "dependencies": {
    "@godaddy/fetch": "^1.0.0",
    "diagnostics": "^2.0.2",
    "jsonp": "^0.2.0",
    "querystringify": "^2.1.1",
    "rip-out": "^1.0.0"
  },
  "publishConfig": {
    "registry": "https://artifactory.secureserver.net/artifactory/api/npm/node-jtsc-local/"
  },
  "engines": {
    "node": ">= 4.x.x"
  }
}
