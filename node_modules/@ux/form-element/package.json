{
  "name": "@ux/form-element",
  "homepage": "https://github.com/gdcorp-uxp/uxcore2/tree/release-2201/packages/legacy/form-element",
  "version": "2201.1.2",
  "gitHead": "c1f5d8ca8874b3565dd9d9d735584140a7e0eabb",
  "description": "Feature-rich form element built on UXCore2",
  "main": "./lib",
  "browser": "./lib",
  "module": "./src",
  "build": "webpack",
  "locales": [
    "en-US"
  ],
  "files": [
    "src",
    "lib",
    "dist",
    "checkbox",
    "criteria",
    "radio",
    "password",
    "search",
    "telephone-input",
    "quantityselector",
    "timepicker",
    "examples",
    "babel.config.js",
    "linaria.config.js",
    "webpack.config.js",
    "wrhs.toml",
    "empty.js",
    ".babelrc.js"
  ],
  "scripts": {
    "build": "npm run build:babel && npm run build:webpack",
    "build-storybook": "build-storybook -o static",
    "build:babel": "babel src -d lib && linaria-to-css && babel src/checkbox.js -o checkbox/index.js && babel src/radio.js -o radio/index.js && babel src/password.js -o password/index.js && babel src/search.js -o search/index.js && babel src/telephone-input.js -o telephone-input/index.js && babel src/quantityselector.js -o quantityselector/index.js && babel src/timepicker.js -o timepicker/index.js && babel src/criteria -d criteria",
    "build:webpack": "webpack",
    "docgen": "ux-react-docgen ./src/**/*.js -e '*/styles.js' -e '*/intents.config.js' -o ./dist/api",
    "lint": "eslint '{src,test,examples}/**/*.js' --ignore-pattern 'test/ui'",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run clean && npm run build && npm run docgen",
    "report": "nyc report --reporter=lcov",
    "storybook": "start-storybook -p 9001",
    "test": "nyc --reporter=text --reporter=json-summary npm run test:web",
    "test:debug": "NODE_ENV=test mocha --debug-brk --inspect --require setup-env ./test/*.test.js",
    "test:web": "NODE_ENV=test mocha --require setup-env test/*.test.js",
    "clean": "rimraf ./lib ./dist",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gdcorp-uxp/uxcore2.git"
  },
  "keywords": [
    "form",
    "element",
    "react"
  ],
  "publishConfig": {
    "registry": "https://artifactory.secureserver.net/artifactory/api/npm/node-uxplatform-local/"
  },
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Arnout Kazemier <akazamier@godaddy.com>",
    "Michael Luther <mluther1@godaddy.com>",
    "Geoffrey Wall <gwall1@godaddy.com>"
  ],
  "contributors": [
    "Martijn Swaagman <mswaagman@godaddy.com>",
    "Keshav Behari <kbehari@godaddy.com>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@ux/button": "^2201.1.2",
    "@ux/component-utilities": "^2201.1.0",
    "@ux/dropdown": "^2201.1.2",
    "@ux/fieldset": "^2201.1.0",
    "@ux/icon": "^2201.1.2",
    "@ux/intents": "^2201.1.0",
    "@ux/stylevars": "^2201.1.0",
    "debounce": "^1.1.0",
    "lodash.escaperegexp": "^4.1.2",
    "react-validation-context": "^6.0.0",
    "rip-out": "^1.0.0"
  },
  "peerDependencies": {
    "linaria": "2.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependenciesMeta": {
    "linaria": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/register": "^7.7.0",
    "@cypress/react": "^5.10.0",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@ux/alert": "^2201.1.2",
    "@ux/babel-config": "^2201.0.0",
    "@ux/linaria-config": "^2201.0.0",
    "@ux/react-docgen": "^2201.1.0",
    "@ux/storybook-config": "^2201.1.0",
    "@ux/style-common": "^2201.1.0",
    "@ux/tooltip": "^2201.1.2",
    "@ux/webpack-config": "^2201.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "assume": "^2.2.0",
    "babel-eslint": "^10.0.3",
    "cypress": "8.7.0",
    "enzyme": "^3.10.0",
    "eslint": "^8.19.0",
    "eslint-config-godaddy-react": "^8.1.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.19.0",
    "jsdom": "^16.5.0",
    "linaria": "2.3.1",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.24.2",
    "rimraf": "3.0.2",
    "setup-env": "^2.0.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy-react",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "ignorePatterns": [
      "checkbox/**/*",
      "criteria/**/*",
      "password/**/*",
      "quantityselector/**/*",
      "radio/**/*",
      "search/**/*",
      "telephone-input/**/*",
      "timepicker/**/*",
      "cypress/**/*.js"
    ],
    "root": false
  },
  "eslintIgnore": [
    "checkbox",
    "criteria",
    "password",
    "quantityselector",
    "radio",
    "search",
    "telephone-input",
    "timepicker"
  ],
  "engines": {
    "node": ">= 14.0.0"
  },
  "exports": {
    "./dossier/api/*": "./dist/api/src/*",
    "./package.json": "./package.json",
    "./dist/styles.css": "./dist/styles.css",
    "./styles": {
      "development": {
        "node": "./empty.js",
        "default": "./dist/styles.css"
      },
      "production": {
        "node": "./empty.js",
        "default": "./dist/styles.css"
      },
      "node": "./empty.js",
      "default": "./dist/styles.css"
    },
    ".": {
      "browser": "./lib/index.js",
      "module": "./src/index.js",
      "default": "./lib/index.js"
    },
    "./quantityselector": "./lib/quantityselector.js",
    "./telephone-input": "./lib/telephone-input.js",
    "./timepicker": "./lib/timepicker.js",
    "./checkbox": "./lib/checkbox.js",
    "./password": "./lib/password.js",
    "./criteria": "./lib/criteria/index.js",
    "./search": "./lib/search.js",
    "./radio": "./lib/radio.js",
    "./src/styles": "./src/styles.js"
  },
  "mochify": {
    "transform": "babelify",
    "reporter": "spec",
    "ui": "bdd"
  }
}
