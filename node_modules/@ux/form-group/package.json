{
  "name": "@ux/form-group",
  "homepage": "https://github.com/gdcorp-uxp/uxcore2/tree/release-2201/packages/legacy/form-group",
  "version": "2201.1.2",
  "gitHead": "c1f5d8ca8874b3565dd9d9d735584140a7e0eabb",
  "description": "Base component to extend when creating groups of FormElements in UXCore2",
  "main": "./lib",
  "browser": "./lib",
  "module": "./src",
  "build": "webpack",
  "locales": [
    "en-US"
  ],
  "files": [
    "src",
    "lib",
    "examples",
    "babel.config.js",
    "webpack.config.js",
    "wrhs.toml",
    "dist",
    "empty.js",
    ".babelrc.js"
  ],
  "scripts": {
    "build": "npm run build:babel && npm run build:webpack && npm run build:css",
    "build-storybook": "build-storybook -o static",
    "build:babel": "babel src -d lib",
    "build:webpack": "webpack",
    "docgen": "ux-react-docgen ./src/**/*.js -e '*/styles.js' -e '*/intents.config.js' -o ./dist/api",
    "lint": "eslint *.js",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run clean && npm run build && npm run docgen",
    "report": "nyc report --reporter=lcov",
    "storybook": "start-storybook -p 9001",
    "test": "nyc --reporter=text --reporter=json-summary mocha --require @babel/register test.js",
    "clean": "rimraf ./lib ./dist",
    "build:css": "mkdir -p dist && cp ./empty.js dist/styles.css",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gdcorp-uxp/uxcore2.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.secureserver.net/artifactory/api/npm/node-uxplatform-local/"
  },
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Arnout Kazemier <akazamier@godaddy.com>",
    "Michael Luther <mluther1@godaddy.com>",
    "Geoffrey Wall <gwall1@godaddy.com>"
  ],
  "contributors": [
    "Martijn Swaagman <mswaagman@godaddy.com>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@ux/button": "^2201.1.2",
    "@ux/form-element": "^2201.1.2",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/register": "^7.7.0",
    "@cypress/react": "^5.10.0",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@ux/babel-config": "^2201.0.0",
    "@ux/react-docgen": "^2201.1.0",
    "@ux/storybook-config": "^2201.1.0",
    "@ux/webpack-config": "^2201.1.0",
    "assume": "^2.2.0",
    "babel-eslint": "^10.0.3",
    "cypress": "8.7.0",
    "eslint": "^8.19.0",
    "eslint-config-godaddy-react": "^8.1.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.19.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "rimraf": "3.0.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "eslintConfig": {
    "extends": "godaddy-react",
    "ignorePatterns": [
      "cypress"
    ],
    "root": false
  },
  "deprecated": "No longer supported - http://go.me/1pW",
  "engines": {
    "node": ">= 14.0.0"
  },
  "exports": {
    "./dossier/api/*": "./dist/api/src/*",
    "./package.json": "./package.json",
    "./dist/styles.css": "./dist/styles.css",
    "./styles": {
      "development": {
        "node": "./empty.js",
        "default": "./dist/styles.css"
      },
      "production": {
        "node": "./empty.js",
        "default": "./dist/styles.css"
      },
      "node": "./empty.js",
      "default": "./dist/styles.css"
    },
    ".": {
      "browser": "./lib/index.js",
      "module": "./src/index.js",
      "default": "./lib/index.js"
    },
    "./src/styles": "./src/styles.js"
  }
}
