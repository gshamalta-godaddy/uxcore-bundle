{
  "description": "Render a UXIcon.\n\n@displayName Icon\n@class Icon\n@public",
  "displayName": "Icon",
  "methods": [],
  "props": {
    "width": {
      "type": {
        "name": "number"
      },
      "required": true,
      "description": "Width of the icon. Applied to the <svg>. element"
    },
    "height": {
      "type": {
        "name": "number"
      },
      "required": false,
      "description": "Height of the icon. Applied to the <svg> element"
    },
    "desc": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Description of the icon. Used as the <desc> element."
    },
    "title": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "Title of the icon, used as <title> element"
    },
    "role": {
      "type": {
        "name": "string"
      },
      "required": false,
      "description": "The role attribute for the svg",
      "defaultValue": {
        "value": "'img'",
        "computed": false
      }
    }
  },
  "composes": [
    "@ux/namespace-component"
  ]
}