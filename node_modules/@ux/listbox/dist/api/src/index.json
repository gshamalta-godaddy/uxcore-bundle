[
  {
    "description": "@param {Object} props - Configuration object\n@returns {Component} - <Listbox/> component",
    "displayName": "Listbox",
    "methods": [],
    "props": {
      "id": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Unique identifier for the component"
      },
      "as": {
        "type": {
          "name": "enum",
          "value": [
            {
              "value": "'listbox'",
              "computed": false
            },
            {
              "value": "'menu'",
              "computed": false
            }
          ]
        },
        "required": false,
        "description": "Determines how the component should behave, namely aria roles used"
      },
      "stretch": {
        "type": {
          "name": "bool"
        },
        "required": false,
        "description": "Controls the stretch property of the underlying sibling set"
      },
      "items": {
        "type": {
          "name": "arrayOf",
          "value": {
            "name": "shape",
            "value": {
              "id": {
                "name": "string",
                "required": true
              },
              "children": {
                "name": "node",
                "required": true
              }
            }
          }
        },
        "required": true,
        "description": "Array of values for the list. As of v2201.0.0 ONLY strings."
      },
      "tabIndex": {
        "type": {
          "name": "union",
          "value": [
            {
              "name": "string"
            },
            {
              "name": "number"
            }
          ]
        },
        "required": false,
        "description": "Override tabIndex. Defaults to 0"
      },
      "onConfirm": {
        "type": {
          "name": "func"
        },
        "required": false,
        "description": "Callback when the item is chosen. The first argument is id of the item."
      },
      "selected": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The selected item id"
      },
      "onActiveItemChange": {
        "type": {
          "name": "func"
        },
        "required": false,
        "description": "Callback for when the active item (aka. the focused item is changed. The first argument is id of the item."
      },
      "loop": {
        "type": {
          "name": "bool"
        },
        "required": false,
        "description": "Enables looping of item selection when using keyboard interaction"
      },
      "anchorRef": {
        "type": {
          "name": "object"
        },
        "required": false,
        "description": "ref used to forward the onKeyDown handler used by Listbox"
      }
    }
  }
]