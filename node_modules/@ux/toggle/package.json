{
  "name": "@ux/toggle",
  "homepage": "https://github.com/gdcorp-uxp/uxcore2/tree/release-2201/packages/components/toggle",
  "version": "2201.1.2",
  "gitHead": "c1f5d8ca8874b3565dd9d9d735584140a7e0eabb",
  "description": "Render a toggle",
  "main": "./lib",
  "browser": "./lib",
  "module": "./src",
  "build": "webpack",
  "locales": [
    "en-US"
  ],
  "files": [
    "src",
    "lib",
    "dist",
    "examples",
    "babel.config.js",
    "linaria.config.js",
    "webpack.config.js",
    "wrhs.toml",
    "empty.js",
    ".babelrc.js"
  ],
  "scripts": {
    "build": "npm run build:babel && npm run build:webpack",
    "build:babel": "babel src -d lib && linaria-to-css",
    "build:webpack": "webpack",
    "cypress:approve": "npm run cypress:run -- --env updateSnapshots=1",
    "cypress:open": "cypress open-ct",
    "cypress:run": "cypress run-ct --headless --browser chrome",
    "docgen": "ux-react-docgen ./src/**/*.js -e '*/styles.js' -e '*/intents.config.js' -o ./dist/api",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint:code": "eslint '{src,examples,test}/**/*.js'",
    "lint:styles": "stylelint src/styles.js",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run clean && npm run build && npm run docgen",
    "storybook": "start-storybook -p 9001",
    "test": "nyc --reporter=text --reporter=json-summary mocha --require setup-env test/*.test.js",
    "clean": "rimraf ./lib ./dist",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gdcorp-uxp/uxcore2.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.secureserver.net/artifactory/api/npm/node-uxplatform-local/"
  },
  "author": "GoDaddy Operating Company, LLC",
  "maintainers": [
    "Lee Glasgow <lglasgow@godaddy.com>",
    "Keshav Behari <kbehari@godaddy.com>"
  ],
  "contributors": [
    "Martijn Swaagman <mswaagman@godaddy.com>",
    "Arnout Kazemier <akazemier@godaddy.com>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@ux/component-utilities": "^2201.1.0",
    "@ux/intents": "^2201.1.0"
  },
  "peerDependencies": {
    "linaria": "2.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependenciesMeta": {
    "linaria": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/register": "^7.7.0",
    "@cypress/react": "^5.10.0",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@ux/alert": "^2201.1.2",
    "@ux/babel-config": "^2201.0.0",
    "@ux/linaria-config": "^2201.0.0",
    "@ux/react-docgen": "^2201.1.0",
    "@ux/storybook-config": "^2201.1.0",
    "@ux/style-common": "^2201.1.0",
    "@ux/webpack-config": "^2201.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "assume": "^2.2.0",
    "babel-eslint": "^10.0.3",
    "cypress": "8.7.0",
    "enzyme": "^3.10.0",
    "eslint": "^8.19.0",
    "eslint-config-godaddy-react": "^8.1.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.19.0",
    "jsdom": "^16.5.0",
    "linaria": "2.3.1",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "3.0.2",
    "setup-env": "^2.0.0",
    "sinon": "^7.5.0",
    "stylelint": "^14.0.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "eslintConfig": {
    "extends": [
      "godaddy-react",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "rules": {
      "strict": 0,
      "callback-return": 0,
      "template-curly-spacing": "off",
      "indent": "off"
    },
    "root": false
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "exports": {
    "./dossier/api/*": "./dist/api/src/*",
    "./package.json": "./package.json",
    "./dist/styles.css": "./dist/styles.css",
    "./styles": {
      "development": {
        "node": "./empty.js",
        "default": "./dist/styles.css"
      },
      "production": {
        "node": "./empty.js",
        "default": "./dist/styles.css"
      },
      "node": "./empty.js",
      "default": "./dist/styles.css"
    },
    ".": {
      "browser": "./lib/index.js",
      "module": "./src/index.js",
      "default": "./lib/index.js"
    },
    "./src/styles": "./src/styles.js"
  }
}
