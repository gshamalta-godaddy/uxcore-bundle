  "scripts": {
    "analyze": "ANALYZE=true DEBUG='ux-webpack*' webpack --progress --bail",
    "analyze:prod": "NODE_ENV=production ANALYZE=true DEBUG='ux-webpack*' webpack --progress --bail",
    "build": "./bin/build",
    "size": "./bin/check-for-built-files && size-limit",
    "size:prod": "rm -rf ./dist && NODE_ENV=production npm run size",
    "compress": "./bin/compress-js && ./bin/compress-css",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run test:coverage && npm run test:bundles",
    "test:web": "mocha --require setup-env test/test.js",
    "test:coverage": "nyc --reporter=text --reporter=json-summary npm run test:web",
    "test:bundle": "mocha --require setup-env test/bundle.test.js && npm run size",
    "test:bundle:prod": "mocha --require setup-env test/bundle-prod.test.js && npm run size:prod",
    "test:bundles": "npm run test:bundle && npm run test:bundle:prod",
    "clean": "rimraf ./lib ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gdcorp-uxp/uxcore2.git"
  },
