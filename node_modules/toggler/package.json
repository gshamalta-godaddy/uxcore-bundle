{
  "name": "toggler",
  "version": "6.0.0",
  "description": "Toggle mixin that switches a state",
  "main": "./lib",
  "browser": "./lib",
  "module": "./index",
  "react-native": "./index",
  "scripts": {
    "test": "nyc --reporter=text --reporter=json-summary npm run mocha",
    "mocha": "mocha --require setup-env test.js",
    "lint": "eslint-godaddy *.js",
    "build": "babel ./index.js -d ./lib",
    "prepublish": "npm run build",
    "posttest": "npm run lint"
  },
  "keywords": [
    "Toggle",
    "mixin",
    "React",
    "component"
  ],
  "author": "GoDaddy.com LLC",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "assume": "^2.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-godaddy": "^3.0.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-mocha": "^5.2.1",
    "mocha": "^5.0.1",
    "nyc": "^13.0.1",
    "setup-env": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.secureserver.net:uxcore2/toggler.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.secureserver.net/artifactory/api/npm/node-uxplatform-local/"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
